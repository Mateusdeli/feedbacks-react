generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Feedbacks {
  id Int @id @default(autoincrement())
  message String
  type String
  date DateTime
  userId Int
  user User @relation(fields: [userId], references: [id])
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String
  device String?
  location String?
  feedbacks Feedbacks?
}